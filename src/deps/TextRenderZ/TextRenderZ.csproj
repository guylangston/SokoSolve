<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net47</TargetFrameworks>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <LangVersion>9</LangVersion>
    <Nullable>enable</Nullable>
 </PropertyGroup>

  <PropertyGroup>
    <Description>Common low-dependancy, low-surface area components across many projects</Description>
    <Authors>Guy Langston</Authors>
    <PackageProjectUrl>https://github.com/guylangston/SokoSolve/tree/master/src/deps/TextRenderZ</PackageProjectUrl>
    <RepositoryUrl>https://github.com/guylangston/SokoSolve/tree/master/src/deps/TextRenderZ</RepositoryUrl>
    <PackageReadmeFile>readme.md</PackageReadmeFile>
    
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Copyright>MIT</Copyright>
    
    <!-- Versioning -->
    <PackageReleaseNotes>move source</PackageReleaseNotes>
    <PackageVersion>0.0.15</PackageVersion>
    <Version>0.0.15</Version>
    <AssemblyVersion>0.0.15</AssemblyVersion>
    <FileVersion>0.0.15</FileVersion>
 </PropertyGroup>

<ItemGroup>
    <None Include="readme.md" Pack="true" PackagePath="\"/>
</ItemGroup>
</Project>
