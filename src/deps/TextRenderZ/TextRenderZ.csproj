<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net47</TargetFrameworks>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <LangVersion>9</LangVersion>
    <Nullable>enable</Nullable>
  </PropertyGroup>
  <PropertyGroup>
    <Description>Common low-dependancy, low-surface area components across many projects</Description>
    <Authors>Guy Langston</Authors>
    <PackageProjectUrl>https://github.com/guylangston/GL.Standard/TextRenderZ</PackageProjectUrl>
    <RepositoryUrl>https://github.com/guylangston/GL.Standard</RepositoryUrl>
    <Copyright>MIT</Copyright>
    <PackageReleaseNotes>Better Cell rendering for unknown</PackageReleaseNotes>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <!-- Versioning -->
    <PackageVersion>0.0.11</PackageVersion>
    <Version>0.0.11</Version>
    <AssemblyVersion>0.0.11</AssemblyVersion>
    <FileVersion>0.0.11</FileVersion>
  </PropertyGroup>
</Project>